    "type": "yii2-extension",
    "keywords": ["yii2","catalog","shop"],
    "license": "MIT",
    "authors": [
        {
            "name": "Evgenii Dudal",
            "email": "wolfstrace@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "^2.0.10",
        "creocoder/yii2-nested-sets":"^0.9.0",
        "robote13/countries-list":"*",
        "robote13/yii2-components":"*",
        "voskobovich/yii2-linker-behavior":"dev-master"
    },
    "require-dev":{
        "yiisoft/yii2-faker": "~2.0.0"
    },
    "autoload": {
        "psr-4": {
            "robote13\\catalog\\": ""
        }
    }
}
